{% block main %}
	<section class="container">
		<p>{{ comment.comment | nl2br }}</p>
	</section>
    <section>
	<h3>Autres commentaires du projet</h3>
	{% if comments is empty %}
		<em>Aucun commentaire pour le moment</em>
	{% else %}
		{% for comment in comments %}
			<div class="panel panel-primary">
				<div class="panel-heading">
					<p><strong>{{ comment.author }}</strong> le {{comment.frenchCreationDate }}</p>  
				</div>
				<div class="panel-body">
					<p>{{ comment.comment | nl2br }}</p>
				</div>
			</div>
		{% endfor %}
	{% endif %}
    </section>
{% endblock %}

{% block script %}
<script>
	$(document).ready(function() {


		var Menu = {

			body: $('.menu'),
			button: $('.button'),
			tools: $('.tools')

		};

		Menu.button.click(function () {
			Menu.body.toggleClass('menu--closed');
			Menu.body.toggleClass('menu--open');
			Menu.tools.toggleClass('tools--visible');
			Menu.tools.toggleClass('tools--hidden');
		});


	});
</script>
{% endblock %}
